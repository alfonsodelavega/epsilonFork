@namespace(uri="picto", prefix="")
package dom;

class Picto {
	attr String template;
	attr String format;
	attr boolean standalone;
	val Model[*] models;
	val Parameter[*] parameters;
}

class Model {
	attr String name;
	attr String type;
	val Parameter[*] parameters;
}

class Parameter {
	attr String name;
	attr String value;
	attr String file;
}